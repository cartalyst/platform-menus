{
	"name": "platform/menus",
	"type": "platform-extension",
	"description": "Manage all the menus throughout your website.",
	"keywords": [
		"cartalyst",
		"platform",
		"laravel",
		"extension",
		"menus"
	],
	"license": "BSD License (3-clause)",
	"authors": [
