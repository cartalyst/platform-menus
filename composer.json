	"repositories": [
		{
			"type": "composer",
			"url": "http://packages.cartalyst.com"
		}
	],
	"require": {
		"php": ">=5.4.0",
		"cartalyst/composer-installers": "1.0.*",
		"cartalyst/nested-sets": "dev-master",
		"platform/admin": "dev-master"
	},
	"autoload": {
		"classmap": [
			"migrations"
