{
    "name": "platform/menus",
    "type": "platform-extension",
    "description": "Manage all the menus throughout your website.",
    "keywords": [
        "cartalyst",
        "platform",
        "laravel",
        "extension",
        "menus"
    ],
    "license": "proprietary",
    "authors": [
        {
            "name": "Cartalyst LLC",
            "email": "help@cartalyst.com",
            "homepage": "https://cartalyst.com"
        }
    ],
    "repositories": [
        { "type": "composer", "url": "https://packages.cartalyst.com" }
    ],
    "require": {
        "php": ">=5.5.9",
        "cartalyst/composer-installers": "1.2.*",
        "cartalyst/nested-sets": "3.0.*",
        "platform/access": "dev-feature/five",
        "platform/foundation": "dev-feature/five"
    },
    "require-dev": {
        "cartalyst/testing": "~2.0@dev",
        "platform/pages": "~3.1"
    },
    "autoload": {
        "classmap": [
            "database/migrations",
            "database/seeds"
        ],
        "psr-4": { "Platform\\Menus\\" : "src/" }
    },
    "extra": {
        "component": "extension",
        "branch-alias": {
            "dev-master": "4.0.x-dev"
        }
    },
    "minimum-stability": "stable"
}
